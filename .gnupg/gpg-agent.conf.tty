
# timeouts in seconds: 3600: 1 hour, 21600: 6 hours, 43200: 12 hours
default-cache-ttl 21600		# How long until timeout after inactivity (like a browser session)
max-cache-ttl 43200		# Maximum total time before requiring re-entry (like forced re-login)

pinentry-program /usr/bin/pinentry-tty
# pinentry-program /usr/bin/pinentry-gnome3
